  "bugs": {
    "url": "https://github.com/chigginss/portfoliosite/issues"
  },
  "homepage": "https://chiggins-portfolio-site.herokuapp.com/",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "jest": "^26.6.3",
    "node": "^15.4.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux": "^4.0.5",
