{
  "name": "portfoliosite",
  "version": "1.0.0",
  "description": "Personal Portfolio Site",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm run start",
    "dev:start": "concurrently --kill-others-on-fail \"npm run client\" \"npm run start\"",
